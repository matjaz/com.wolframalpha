{
  "id": "com.wolframalpha.knowledge",
  "version": "1.3.1",
  "compatibility": "0.x || 1.x",
  "category": [
    "internet"
  ],
  "name": {
    "en": "WolframAlpha",
    "nl": "WolframAlpha"
  },
  "description": {
    "en": "This is the WolframAlpha app for Homey",
    "nl": "Dit is de WolframAlpha app voor Homey"
  },
  "author": {
    "name": "Athom B.V."
  },
  "contributors": {
    "developers": [
      {
        "name": "Matjaž Lipuš",
        "website": "http://matjaz.info",
        "email": "matjazl@gmail.com"
      }
    ]
  },
  "images": {
    "large": "./assets/images/large.jpg",
    "small": "./assets/images/small.jpg"
  },
  "dependencies": {
    "node-wolfram": "*",
    "http": "*",
    "wav": "*",
    "speaker": "*",
    "stream": "*"
  },
  "media": [
    "play",
    "pause"
  ],
  "permissions": [
    "homey:manager:media",
    "homey:manager:speech-input",
    "homey:manager:speech-output"
  ],
  "flow": {
    "triggers": [],
    "conditions": []
  },
  "speech": [
    {
      "id": "wolfram",
      "importance": 0.9,
      "triggers": {
        "en": [
          "wolfram",
          "wolfram alpha"
        ],
        "nl": [
          "wolfram",
          "wolfram alpha"
        ]
      }
    },
    {
      "id": "question",
      "importance": 0.8,
      "triggers": {
        "en": [
          "question",
          "i have a question"
        ],
        "nl": [
          "vraag",
          "ik heb een vraag",
          "ik heb een vraagje"
        ]
      }
    },
    {
      "id": "what",
      "importance": 0.5,
      "triggers": {
        "en": [
          "what"
        ],
        "nl": [
          "wat"
        ]
      }
    }
  ]
}